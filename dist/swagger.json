    "definitions": {
        "User": {
            "properties": {
                "user_name": {
                    "type": "string",
                    "description": "User name of Knowledge 360 user (requested  for add)."
                },
                "password": {
                    "type": "string",
                    "description": "password for user (only required for add or to update password)"
                },
                "authority": {
                    "type": "string",
                    "description": "Provides access control (can be either Student or Administrator or Teacher)",
                    "enum": [
                        "Student",
                        "Administrator",
                        "Teacher"
                    ]
                },
                "first_name": {
                    "type": "string",
                    "description": "first name of the Knowledge 360 user"
                },
                "sur_name": {
                    "type": "string",
                    "description": "last (or surname) of the Knowledge 360 user"
                },
                "organization_name": {
                    "type": "string",
                    "description": "organization in Knowledge 360 that the user will be part of"
                },
                "email_address": {
                    "type": "string",
                    "description": "valid email address"
                },
                "phone_number": {
                    "type": "string",
                    "description": "phone number of user without any non-numeric characters (ex. 3035551212)"
                },
                "locale": {
                    "type": "string",
                    "description": "currently en-US only",
                    "enum": [
                        "en-US"
                    ]
                }
            }
        },
        "Error": {
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32",
                    "description": "code indicating whether the request was successfully or not."
                },
                "message": {
                    "type": "string",
                    "description": "text string that contains the message that further describes success or failure condition."
                },
                "fields": {
                    "type": "string",
                    "description": "contains a comma delimited string that shows what field is in error"
                }
            }
        },
        "Course": {
            "properties": {
                "course_name": {
                    "type": "string",
                    "description": "name of course for Knowledge 360"
                },
                "course_id": {
                    "type": "string",
                    "description": "id of course for Knowledge 360"
                },
                "course_description": {
                    "type": "string",
                    "description": "summary description of course to be used as a foundation for any badge descriptions"
                },
                "course_url": {
                    "type": "string",
                    "description": "full url to link course in Knowledge 360 to an existing web presence for a course"
                },
                "course_program_area": {
                    "type": "string",
                    "description": "main category for the course to be used to organization courses and related badges into a catalog"
                },
                "course_sub_program": {
                    "type": "string",
                    "description": "sub category to be used with a category"
                },
                "course_delivery_location": {
                    "type": "string",
                    "description": "information that indicates how the training will be taken",
                    "enum": [
                        "Online",
                        "Classroom",
                        "Mixed",
                        "Mail"
                    ]
                }
            }
        },
        "Outcome": {
            "properties": {
                "outcome_type": {
                    "type": "string",
                    "description": "indicates whether outcome is tied to a course or a badge (non-course based badges)",
                    "enum": [
                        "Course",
                        "Badge"
                    ]
                },
                "course_badge_name": {
                    "type": "string",
                    "description": "name of course or badge in Knowledge 360. Course or badge must exist for outcome object to be added."
                },
                "sequence_number": {
                    "type": "integer",
                    "format": "int32",
                    "description": "a number that specifies the sequence that the outcomes are listed on the Badge and Course descriptions"
                },
                "outcome_name": {
                    "type": "string",
                    "description": "short name of the outcome"
                },
                "outcome_description": {
                    "type": "string",
                    "description": "long description of the outcome"
                }
            }
        },
        "Badge": {
            "properties": {
                "badge_type": {
                    "type": "string",
                    "description": "the type of badge that indicates how the badge will be created and managed. No course required creates the badge directly in Knowledge 360. Course based uses an existing course to model the badge name, description and outcomes from. Course pending is like no course required, but leaves open the opportunity to hook the badge to a course at a later date. Activity based badges will not be associated to a course and are designed to be given for accomplishing a specific activity.",
                    "enum": [
                        "No Course Rqd",
                        "Course Based",
                        "Course Pending",
                        "Activity Based"
                    ]
                },
                "course_name": {
                    "type": "string",
                    "description": "used only with course-based badges and must be amn existing Knowledge 360 course name"
                },
                "badge_name": {
                    "type": "string",
                    "description": "used with no course required, course pending, and activity badges and must not currently be an exisitng badge name."
                },
                "badge_description": {
                    "type": "string",
                    "description": "used only with no course required, course pending, and activity badges. Must be blank for coursed-based badges."
                },
                "user_organization": {
                    "type": "string",
                    "description": "must be an existing Knowledge 360 organization and will reflect the group the user is associated with"
                },
                "badge_level": {
                    "type": "string",
                    "description": "indicates a what level in a badging hierarchy that the badge is placed",
                    "enum": [
                        "Journey",
                        "Quest",
                        "Trek"
                    ]
                },
                "parent_badge": {
                    "type": "string",
                    "description": "ties a new badge to an existing badge for parentage. If no parent is needed, the value of None must be included otherwise include a valid Knowledge 360 badge"
                },
                "life_cycle_option": {
                    "type": "string",
                    "description": "indicates whether and how badge expirations should be handled",
                    "enum": [
                        "No Expiration",
                        "Duration",
                        "Fixed Date"
                    ]
                },
                "life_cycle_period": {
                    "type": "integer",
                    "format": "int32",
                    "description": "used only for badges with life cycle options of duration. number of months from award date that badge expires. Option periods are current set at 6, 12 ,24 ,36, 48 and 60 months",
                    "enum": [
                        6,
                        12,
                        24,
                        36,
                        48,
                        60
                    ]
                },
                "life_cycle_expiration": {
                    "type": "string",
                    "description": "only required for Fixed Date badges and reflects date that badge should expire no matter when it was issued. Must be a date in the future in the format yyyy-mm-dd."
                }
            }
        },
        "OutcomeKey": {
            "properties": {
                "course_badge_name": {
                    "type": "string",
                    "description": "name of course or badge in Knowledge 360. Course or badge must exist for outcome object to be added."
                },
                "outcome_name": {
                    "type": "string",
                    "description": "short name of the outcome"
                }
            }
        },
        "BadgeStatus": {
            "properties": {
                "user_name": {
                    "type": "string",
                    "description": "User name of Knowledge 360 user (requested  for add)."
                },
                "badge_name": {
                    "type": "string",
                    "description": "used with no course required, course pending, and activity badges and must not currently be an exisitng badge name."
                },
                "badge_status": {
                    "type": "integer",
                    "format": "int32",
                    "description": "badge status is used to advance the life cycle of a badge. The statuses are Registered (1), InProgress(2), Earned(3), Awarded(4), Criteria Not Met(5) and Dropped(6). One other status is supported in Knowledge 360 of Planned (0), but is not supported in the current API. Currently, the API only supports moving badge statuses forward one step at a time.",
                    "enum": [
                        1,
                        2,
                        3,
                        4,
                        5,
                        6
                    ]
                },
                "date_awarded": {
                    "type": "string",
                    "description": "only required for a Awarded status change, otherwise should be blank.  This reflects the date that badge was actually awarded on and must be a date in the future in the format yyyy-mm-dd."
                }
            }
        },
        "BadgeInstance": {
            "properties": {
                "badge_id": {
                    "type": "integer",
                    "format": "int64",
                    "description": "key assigned to badge instance (a user's specific badge) by Knowledge 360."
                },
                "user_name": {
                    "type": "string",
                    "description": "user name of badge from Knowledge 360."
                },
                "user_first_name": {
                    "type": "string",
                    "description": "first name of user with badge from Knowledge 360."
                },
                "user_sur_name": {
                    "type": "string",
                    "description": "sur (or last) name of user with badge from Knowledge 360."
                },
                "badge_name": {
                    "type": "string",
                    "description": "badge name set up in Knowledge 360"
                },
                "badge_description": {
                    "type": "string",
                    "description": "badge description set up in Knowledge 360"
                },
                "user_organization": {
                    "type": "string",
                    "description": "the existing Knowledge 360 organization assigned to the user"
                },
                "badge_level": {
                    "type": "string",
                    "description": "indicates a what level in a badging hierarchy that the badge is",
                    "enum": [
                        "Journey",
                        "Quest",
                        "Trek"
                    ]
                },
                "badge_status_name": {
                    "type": "string",
                    "description": "badge status is used to advance the life cycle of a badge. The statuses are Planned (0), Registered (1), InProgress(2), Earned(3), Awarded(4), Criteria Not Met(5) and Dropped(6).",
                    "enum": [
                        "Planned",
                        "Registered",
                        "InProgress",
                        "Earned",
                        "Awarded",
                        "Criteria Not Met",
                        "Dropped"
                    ]
                },
                "date_awarded": {
                    "type": "string",
                    "description": "Only included if badge status is Awarded. This field will contain the date (yyyy-mm-dd) the badge was issued."
                },
                "life_cycle_expiration": {
                    "type": "string",
                    "description": "Only included if badge status is Awarded and has a life cycle option that sets an expiration date. This shows when a badge will expired (yyyy-mm-dd)."
                }
            }
        }
    }
}